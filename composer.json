{
  "name": "edofre/laravel-sluggable",
  "description": "Generate slugs when saving Eloquent models",
  "keywords": [
    "spatie",
    "laravel-sluggable"
  ],
  "homepage": "https://github.com/edofre/laravel-sluggable",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/database": "~5.0.0|~5.1.0|~5.2.0|~5.3.0|~5.4.0",
    "illuminate/support": "~5.0.0|~5.1.0|~5.2.0|~5.3.0|~5.4.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.2.0|~3.3.0",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Edofre\\Sluggable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Edofre\\Sluggable\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
